package makeNumberOne;

import java.util.Arrays;
import java.util.Scanner;

public class 영역구하기2 {

	static int paper[][];
	static int dx[] = { 0, 0, 1, -1 };
	static int dy[] = { 1, -1, 0, 0 };
	static int M;
	static int N;
	static int result[];

	static void paint(int i, int j, int color) {
		paper[i][j] = color;
		result[color]++;
		for (int ar = 0; ar < 4; ar++) {
			if (i + dx[ar] >= 0 && j + dy[ar] >= 0 && i + dx[ar] < M && j + dy[ar] < N) {
				if(paper[i+dx[ar]][j+dy[ar]] == 0){
					paint(i+dx[ar],j+dy[ar],color);
				}
			}
		}
	}

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		result = new int[3000];
		M = sc.nextInt();
		N = sc.nextInt();
		int k = sc.nextInt();
		paper = new int[M][N];
		
		for(int i=0;i<k;i++){
			int y1 = sc.nextInt(); int x1 = sc.nextInt();
			int y2 = sc.nextInt(); int x2 =sc.nextInt();
			
			for(int m=x1;m<x2;m++){
				for(int n=y1;n<y2;n++){
					paper[m][n] = 1;
				}
			}
		}// input
		int color = 1;
		for(int i=0;i<M;i++){
			for(int j=0;j<N;j++){
				if(paper[i][j] == 0){
					paint(i,j,color);
					color++;
				}
			}
		}
		System.out.println(color-1);
		Arrays.sort(result,1,color);
		for(int i=1;i<color;i++){
			System.out.print(result[i]+"");
		}
	}

}
