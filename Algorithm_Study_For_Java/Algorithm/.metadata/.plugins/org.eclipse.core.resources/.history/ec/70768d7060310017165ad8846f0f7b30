package algorithm_week1;

import java.util.Scanner;

public class min_max_algorithm {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.println("입력할 수 갯수");
		int num = sc.nextInt();
		int val[] = new int[num];
		int cb = 0;
		int temp;

		while (num > 0) {
			val[num - 1] = sc.nextInt();
			num--;
		}
		while (num > 0) {
			if (val[cb] > val[cb + 1]) {
				temp = val[cb + 1];
				val[cb + 1] = val[cb];
				val[cb] = temp;
			}else if(val[cb] < val[cb+1]){
				// nothing changed
			}
			cb++;
		}
		System.out.println(val[cb]);
	}
}
