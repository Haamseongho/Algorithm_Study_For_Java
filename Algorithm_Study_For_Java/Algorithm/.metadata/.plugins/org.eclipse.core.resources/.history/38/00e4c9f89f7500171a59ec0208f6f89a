public class TapeEquilibrium {

	static int solution(int[] A) {
		int result = A.length + 1;
		for (int i = 1; i <= A.length; i++) {
			result += i - A[i - 1];
		}
		return result;
	}

	public static void main(String[] args) {
		int arr[] = { 3, 1, 2, 4, 3 };
		System.out.print(solution(arr));
	}

}
